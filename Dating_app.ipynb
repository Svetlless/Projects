      "text/plain": [
       "BootstrapResult(confidence_interval=ConfidenceInterval(low=0.16666666666666666, high=0.2), standard_error=0.010987509573202813)"
      ]
     },
     "execution_count": 33,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "bootstrap((control, ), np.median, method = 'percentile')"
   ]
